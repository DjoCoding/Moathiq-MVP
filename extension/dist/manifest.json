{
    "manifest_version": 3,
    "name": "Moathiq Extension",
    "description": "A Chrome extension that serves the Moathiq platform",
    "version": "1.0",
    "permissions": [
      "tabCapture",
      "storage",
      "activeTab",
      "scripting"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html"
    }
}
  